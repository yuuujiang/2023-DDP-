def speeding_ticket(speed, is_birthday):
    no_ticket_limit = 60
    small_ticket_limit = 80

   
    if is_birthday:
        no_ticket_limit += 5
        small_ticket_limit += 5

    if speed <= no_ticket_limit:
        return "No Ticket"
    elif speed <= small_ticket_limit:
        return "Small Ticket"
    else:
        return "Big Ticket"
