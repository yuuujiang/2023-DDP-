def is_prime(number):
    if number < 2:
        return False
    for i in range(2, int(number**0.5) + 1):
        if number % i == 0:
            return False 
    return True  # Number is prime

# Test cases
print(is_prime(11))  # True
print(is_prime(4))   # False
print(is_prime(2))   # True
print(is_prime(1))   # False
